{
  "name": "tv-show-scores",
  "builds": [
    { "src": "dist/tv-show-scores/browser/**/*", "use": "@vercel/static" },
    {
      "src": "dist/tv-show-scores/server/main.server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "dist/tv-show-scores/server/main.server.mjs" }
  ]
}
