{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "public": true,
  "name": "tv-show-scores",
  "devCommand": "npm run serve:ssr:tv-show-scores",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/tv-show-scores/browser/**"
    }
  }
}
